{
  "name": "terraphim-it",
  "displayName": "Terraphim It",
  "description": "Apply Current Terraphim AI role to the current text",
  "version": "0.0.1",
  "publisher": "applied-knowledge-systems",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/terraphim/terraphim-ai"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.terraphimCommand",
        "title": "Terraphim IT"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "pnpm exec tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "pnpm exec tsc -watch -p ./",
    "update-ontologies": "pnpm exec ad-generate ontologies"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^7.14.0",
    "@typescript-eslint/parser": "^7.14.0",
    "eslint": "^8.26.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@tomic/cli": "0.40.0",
    "@tomic/lib": "0.40.0"
  }
}

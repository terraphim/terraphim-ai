{
  "name": "terraphim-ai-workflow-tests",
  "version": "1.0.0",
  "description": "Browser automation tests for Terraphim AI multi-agent workflow integration",
  "main": "browser-automation-tests.js",
  "scripts": {
    "test": "node browser-automation-tests.js",
